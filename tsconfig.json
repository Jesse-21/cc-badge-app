    "include": ["./src/**/*"],
    "compilerOptions": {
        "incremental": true,
        "allowJs": true,
        "skipLibCheck": true,
        "lib": ["dom", "es2015"],
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "jsx": "react-jsx"
    }
}
